<?xml version="1.0" encoding="UTF-8"?>
<configuration scan="true">
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
      <encoder>
        <pattern>%d [%t] %p %t %c{40} - %m%n</pattern>
      </encoder>
    </appender>
        
    <logger name="com.nordea.next" level="INFO"/>    
    <logger name="com.nordea.dompap" level="INFO"/>
    <logger name="com.nordea.dompap.workflow.WorkflowTimer" level="ERROR"/>
    
    <root level="DEBUG">
      <appender-ref ref="CONSOLE" />
    </root>
</configuration>